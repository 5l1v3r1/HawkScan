<script>
	// Phone validation for contact and demo request fields
	$(document).ready(function() {
		if ($('body').hasClass('contact') || $('body').hasClass('demoRequest')) {
			$('body').on('keyup', "input[name='Phone']", function() {				
				var val = $(this).val().replace(/[^\d().-]/g, "");
				$(this).val(val);
			});

			MktoForms2.whenReady(function(form) {
				if ((form.getId() === 1029 && $('body').hasClass('demoRequest')) || (form.getId() === 1080 && $('body').hasClass('contact'))) {
					form.onSubmit(function() {
						var phone = form.getValues().Phone;
						var phoneLength = phone.replace(/\D+/g, "").length;
						var email = form.getValues().Email;
						var hasValidPhoneNumber = true;
						var hasValidEmail = true;

						if (phoneLength < 10 || phoneLength > 13) {
							hasValidPhoneNumber = false;
							form.showErrorMessage("Phone number must be between 10 and 13 digits.", form.getFormElem().find("input[name='Phone']"));
						}
						
						if (!/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(email)) {
							hasValidEmail = false;
							form.showErrorMessage("Must be a valid email.<br />example@yourdomain.com", form.getFormElem().find("input[name='Email']"));
						}

						if (hasValidEmail && hasValidPhoneNumber) {
							form.submittable(true);
							return true;
						}

						form.submittable(false);
						return false;
					})
				}
			})
		}
	});
</script>
